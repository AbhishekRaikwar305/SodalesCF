COLUMN TABLE "INC_T_INCDT" (
    "ID"    BIGINT  PRIMARY KEY GENERATED ALWAYS AS IDENTITY (START WITH 1 INCREMENT BY 1) COMMENT 'EMPLOYEE ID',
    "NAME"  NVARCHAR(100) COMMENT 'EMPLOYEE NAME',
    "CITY"  NVARCHAR(100) COMMENT 'EMPLOYEE CITY',
    "ISDEL" NVARCHAR(1) COMMENT 'ISDEL FLAG 0 AND 1',
    "CRTBY" NVARCHAR(100) COMMENT 'CREATED BY',
    "CRTDT" DATE COMMENT 'CREATED DATE',
    "CRTTM" TIME COMMENT 'CREATED TIME'
)

