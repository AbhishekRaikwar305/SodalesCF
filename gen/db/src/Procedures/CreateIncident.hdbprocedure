PROCEDURE "CreateIncident"( 
    IN ID INTEGER,
    IN NAME NVARCHAR(100),
    IN CITY NVARCHAR(100)
)

   LANGUAGE SQLSCRIPT
   SQL SECURITY INVOKER
   --DEFAULT SCHEMA <default_schema_name>
--    READS SQL DATA
 AS
BEGIN
    
    INSERT INTO "INC_T_INCDT" ( NAME, CITY, ISDEL, CRTBY, CRTDT, CRTTM)
    VALUES ( NAME, CITY, '0' , SESSION_user, CURRENT_DATE, CURRENT_TIME);
END